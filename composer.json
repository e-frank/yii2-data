{
    "name":        "e-frank/yii2-data",
    "description": "Helpers for persisting relational data from arrays",
    "keywords":    ["yii", "ActiveRecord", "relations", "hierarchical"],
    "type":        "yii2-extension",
    "license":     "BSD-3-Clause",
    "authors": [
        {
            "name"    : "Elmar Frank",
            "homepage": "http://yii.efrank.at/extension/data"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable":     true,
    "version":           "1.1.0",
    "require":           {
        "php":          ">=5.4.0",
        "yiisoft/yii2": "~2.0.6"
    },
    "autoload": {
        "psr-4": { "x1\\data\\": "" }
    },
    "extra": {
        "bootstrap": "x1\\data\\DataBootstrap"
